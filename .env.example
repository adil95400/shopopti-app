# Exemple de configuration pour Shopopti+

# OpenAI
VITE_OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Shopify
VITE_SHOPIFY_DOMAIN=your-store.myshopify.com
VITE_SHOPIFY_ADMIN_TOKEN=YOUR_SHOPIFY_ADMIN_TOKEN

# Stripe
VITE_STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY
VITE_STRIPE_PRICE_PRO=PRICE_EXAMPLE_ID

# 17Track API (Package Tracking)
VITE_17TRACK_API_KEY=YOUR_17TRACK_API_KEY
VITE_17TRACK_API_SECRET=YOUR_17TRACK_API_SECRET

# Canva
VITE_CANVA_APP_ID=YOUR_CANVA_APP_ID

# Langue
VITE_DEFAULT_LANGUAGE=your_language_code
